{
    "version": "1",
    "manifest_version": 2,
    "name": "Whatsapp Web Dark Mode Enabler",
    "description": "Enable hidden dark mode in Whatsapp Web",
    "author": "Fernando Moreno Moran",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon16.png"
    },
    "content_scripts": [{
        "matches": [
            "*://web.whatsapp.com/*"
        ],
        "js": [
            "popup.js"
        ]
    }]
}